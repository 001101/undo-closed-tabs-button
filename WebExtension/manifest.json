{
  "name": "Undo Closed Tabs Button",
  "short_name": "iuctbutton",
  "description": "Add a toolbar button onto any toolbar to undo closed tabs",
  "author": "Andy Portmen",
  "version": "0.1.6",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "sessions",
    "notifications",
    "contextMenus",
    "chrome://favicon/*",
    "browsingData"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "common.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "64": "data/icons/64.png"
    },
    "default_popup": "data/popup/index.html"
  },
  "homepage_url": "http://mybrowseraddon.com/undo.html",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  },
  "applications": {
    "gecko": {
      "id": "{5997e7bd-1940-4058-a5f4-1562afce6353}",
      "strict_min_version": "52.0"
    }
  }
}
